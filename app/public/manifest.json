{
  "name": "Boocasta Bookmark Manager",
  "description": "A better and faster version",
  "version": "0.1",
  "options_page": "index.html",
  "manifest_version": 3,
  "incognito": "split",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "bookmarks"
  ],
  "chrome_url_overrides": {
    "PAGE_TO_OVERRIDE": "bookmarks.html"
  },
  "omnibox": {
    "keyword": "bk/"
  },
  "short_name": "bkmrk",
  "author": "stan-dot",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup",
    "default_icon": {
      "16": "/images/favicon-16x16.png",
      "32": "/images/favicon-32x32.png"
    }
  }
}