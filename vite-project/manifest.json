{
  "name": "BKMRK Bookmark Manager",
  "description": "A better and faster version",
  "version": "0.1",
  "options_page": "index.html",
  "manifest_version": 3,
  "incognito": "split",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "bookmarks"
  ],
  "omnibox": {
    "keyword": "bk/"
  },
  "short_name": "bkmrk",
  "author": "stan-dot",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  }
}